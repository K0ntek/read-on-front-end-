<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="UTF-8" />
        <meta http-equiv="X-UA-Compatible" content="IE=edge" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        <title>Logi</title>
    </head>
    <body>
        <h1>mmmmm logs</h1>
        <table>
            <tr>
                <td>#</td>
                <td>Time</td>
                <td>Type</td>
                <td>Additional data üçª</td>
            </tr>
            <% logs.forEach((log, i) => { %>
            <tr>
                <td><%= i + 1 %></td>
                <td><%= log.created_at %></td>
                <td><%= log.type %></td>
                <td><%= log.data %></td>
            </tr>
            <% }) %>
        </table>
    </body>
</html>
